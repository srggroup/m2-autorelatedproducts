{
    "name": "srggroup/m2-autorelatedproducts",
    "type": "magento2-module",
    "require": {
        "php": "^8.1",
        "magento/module-backend": "^102.0",
        "magento/module-catalog": "^104.0"
    },
    "require-dev": {
        "srggroup/srg_standards": "dev-master"
    },
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Srg\\AutoRelatedProducts\\": ""
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
